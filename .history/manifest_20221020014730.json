{
  "name": "Title picker",
  "description": "  A title picker extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/graphic_design (3).png",
    "32" :"/images/graphic_design (2).png",
    "48" : "/images/graphic_design.png"
  },
  "background":{
    "service_worker":"background.js"
},


"permissions":[
  "storage",
  "activeTab",
  "scripting"
],
"action":{
  "default_popup":"popup.html",
  "16": "/images/graphic_design (3).png",
  "32" :"/images/graphic_design (2).png",
  "48" : "/images/graphic_design.png"
}



}